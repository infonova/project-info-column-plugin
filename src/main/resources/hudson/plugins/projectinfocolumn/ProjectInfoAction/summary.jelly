<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
    <j:forEach var="infoEntry" items="${it.projectInfos}">
        <t:summary icon="${infoEntry.icon}">
            <b>${infoEntry.title}</b>
            ${infoEntry.value}
        </t:summary>
    </j:forEach>
</j:jelly>
